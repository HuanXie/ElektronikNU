<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="A">#356289</color>
    <color name="C">#FFFFFF</color>
    <string name="B">center</string>

    <style name="CustomTheme" parent="@style/Theme.AppCompat.Light">
        <item name="actionBarStyle">@style/MyActionBar</item>
    </style>

    <style name="MyActionBar" parent="@style/Widget.AppCompat.ActionBar">
        <item name="background">@color/A</item>
        <item name="titleTextStyle">@style/MyTitleTextStyle</item>
    </style>

    <style name="MyTitleTextStyle" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/C</item>
    </style>

</resources>